{
    "name": "becklyn/php-cs",
    "type": "lib",
    "description": "Config files for PHP code style",
    "homepage": "https://github.com/Becklyn/PHP-Code-Style",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-json": "*",
        "friendsofphp/php-cs-fixer": "^v3.2.1",
        "phpstan/phpstan": "^1.0.0",
        "phpstan/phpstan-doctrine": "^1.0.0",
        "phpstan/phpstan-symfony": "^1.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\PhpCs\\": "src/"
        }
    }
}
